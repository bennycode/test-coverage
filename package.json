{
  "dependencies": {
    "@types/node": "10.0.4"
  },
  "devDependencies": {
    "awesome-typescript-loader": "5.0.0",
    "cross-env": "5.1.5",
    "istanbul": "1.1.0-alpha.1",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "3.1.0",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "remap-istanbul": "0.11.1",
    "rimraf": "2.6.2",
    "typescript": "2.8.3",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3"
  },
  "license": "ISC",
  "main": "./dist/commonjs/MyClass.js",
  "name": "test-coverage",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf coverage dist",
    "cov:browser": "karma start",
    "cov:bundle": "node bundle-coverage.js",
    "cov:node:cover": "istanbul cover ./node_modules/jasmine/bin/jasmine.js --print detail --report html",
    "cov:node:cover:remap": "remap-istanbul -i ./coverage/node/cover/coverage.raw.json -o ./coverage/node/remap/coverage-node.json",
    "cov:node:cover:remap:html": "remap-istanbul -i ./coverage/node/cover/coverage.raw.json -o ./coverage/node/remap/html -t html",
    "cov:node:cover:report": "istanbul report --root ./coverage/node/cover --dir ./coverage/node/report html",
    "cov:report": "istanbul report --root ./coverage/report --dir ./coverage/report/html html",
    "dist": "yarn clean && yarn build && yarn pack",
    "pack": "webpack --verbose",
    "test": "yarn test:node && yarn test:browser",
    "test:browser": "karma start",
    "test:node": "jasmine"
  },
  "version": "1.0.0"
}
