{
  "dependencies": {
    "@types/node": "10.0.4"
  },
  "devDependencies": {
    "awesome-typescript-loader": "5.0.0",
    "cross-env": "5.1.4",
    "istanbul": "1.1.0-alpha.1",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "3.1.0",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "rimraf": "2.6.2",
    "typescript": "2.8.3",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3"
  },
  "license": "ISC",
  "main": "./dist/commonjs/MyClass.js",
  "name": "test-coverage",
  "scripts": {
    "b": "tsc",
    "c": "rimraf coverage dist",
    "cov": "yarn cov:browser && yarn cov:node && yarn cov:report",
    "cov:browser": "karma start",
    "cov:node": "istanbul cover ./node_modules/jasmine/bin/jasmine.js --print detail --report html",
    "cov:report": "istanbul report --root ./coverage --dir ./coverage/report html",
    "d": "yarn c && yarn b && yarn p",
    "dt": "yarn d && yarn t",
    "p": "webpack --verbose",
    "t": "yarn t:node && yarn t:browser",
    "t:node": "jasmine",
    "t:browser": "karma start"
  }
}
